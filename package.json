{
  "name": "webadmin-middleware",
  "version": "1.0.0",
  "description": "webadmin middleware for axa webadmin",
  "private": true,
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "local": "npm run build && npm run start",
    "build": "node node_modules/webpack/bin/webpack.js --config config/webpack.config.js",
    "postinstall": "node node_modules/typings/dist/bin.js install --overwrite",
    "start": "node lib/build.js"
  },
  "main": "lib/build.js",
  "keywords": [
