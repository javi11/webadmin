    "webpack": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.1",
    "express": "^4.15.2",
    "express-http-proxy": "^1.0.3",
    "morgan": "^1.7.0"
  }
}